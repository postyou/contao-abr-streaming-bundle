{
    "name": "postyou/contao-abr-streaming-bundle",
    "description": "Adaptive Bitrate Streaming using Video.js",
    "type": "contao-bundle",
    "license": "MIT",
    "keywords": [
        "contao",
        "streaming",
        "dash",
        "hls"
    ],
    "authors": [
        {
            "name": "Postyou Werbeagentur",
            "homepage": "https://postyou.de"
        }
    ],
    "support": {
        "issues": "https://github.com/postyou/contao-abr-streaming-bundle/issues",
        "source": "https://github.com/postyou/contao-abr-streaming-bundle"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/http-kernel": "^5.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "contao/easy-coding-standard": "^6.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Postyou\\ContaoABRStreamingBundle\\": "src/"
        },
        "classmap": [
            "contao/elements/ContentVideoJsPlayer.php"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Postyou\\ContaoABRStreamingBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": true,
            "contao/manager-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
